{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::cloudsec.bucket",
                "arn:aws:s3:::cloudsec.bucket/*"
            ]
        }
    ]
}
 #List, Permissions management, Read, Write, Tagging
#This policy provides full control over specific S3 bucket actions, like listing objects, uploading, and deleting objects while allowing bucket-level permissions management (e.g., bucket ACLs, policies).
